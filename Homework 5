	<script>
		const twiceOld = (fatherAge, sonAge) => Math.abs(sonAge * 2 - fatherAge)
		console.log(twiceOld(55, 20));
		
		
		
		
		function oddCount(n){
			return Math.floor(n/2);
		}
		console.log(oddCount(17));
		
		
		
		
		const stringToNumber = 	function(str){
			return +str;
		}
		console.log(stringToNumber('15738'));
	</script>
